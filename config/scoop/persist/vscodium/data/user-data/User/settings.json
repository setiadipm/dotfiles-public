{
    // START FVM CONFIG
    "dart.flutterSdkPath": ".fvm/flutter_sdk",
    "files.watcherExclude": {
        "**/.fvm": true
    },
    "search.exclude": {
        "**/.fvm": true
    },
    // END FVM CONFIG
    "dart.openDevTools": "never",
    "dart.previewLsp": true,
    "dart.showInspectorNotificationsForWidgetErrors": false,
    "dashboard.projectData": null,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.accessibilitySupport": "off",
    "editor.bracketPairColorization.enabled": true,
    "editor.defaultFormatter": null,
    "editor.fontFamily": "JetBrainsMono Nerd Font Mono",
    "editor.fontLigatures": true,
    // "editor.formatOnSave": true,
    // "editor.formatOnPaste": true,
    "editor.fontSize": 13,
    "editor.guides.bracketPairs": true,
    "editor.minimap.enabled": false,
    "editor.lineHeight": 20,
    "editor.lineNumbers": "relative",
    "editor.snippetSuggestions": "top",
    "editor.suggestLineHeight": 20,
    "explorer.openEditors.visible": 0,
    "files.associations": {
        "*.dart": "dart"
    },
    "git.confirmSync": false,
    "go.lintTool": "golangci-lint",
    "go.lintFlags": [
        "--fast"
    ],
    "go.toolsManagement.autoUpdate": true,
    "html.format.wrapAttributes": "force-expand-multiline",
    "intelephense.environment.phpVersion": "7.4.16",
    "liveSassCompile.settings.formats": [
        {
            "format": "compressed",
            "extensionName": ".min.css",
            "savePath": "/css"
        }
    ],
    "liveServer.settings.donotShowInfoMsg": true,
    "namespaceResolver.sortAlphabetically": true,
    "php.suggest.basic": false,
    "php.validate.executablePath": "D:\\Apache24\\php74\\php.exe",
    "prettier.singleQuote": true,
    "redhat.telemetry.enabled": false,
    "terminal.integrated.cursorBlinking": false,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font Mono",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.profiles.windows": {
        "Git Bash": {
            "source": "Git Bash",
            "args": ["--login"]
        }
    },
    "terminal.integrated.tabs.enabled": true,
    "typescript.preferences.importModuleSpecifier": "relative",
    "typescript.preferences.quoteStyle": "single",
    "vim.leader": "<space>",
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.handleKeys": {
        "<C-w>": false,
    },
    "vim.normalModeKeyBindings": [
        {
            "before": ["<C-u>"],
            "after": ["<C-u>", "z", "z"]
        },
        {
            "before": ["<C-d>"],
            "after": ["<C-d>", "z", "z"]
        },
        {
            "before": ["<Up>"],
            "after": ["<NOP>"]
        },
        {
            "before": ["<Down>"],
            "after": ["<NOP>"]
        },
        {
            "before": ["<Left>"],
            "after": ["<NOP>"]
        },
        {
            "before": ["<Right>"],
            "after": ["<NOP>"]
        },
        // {
        //     "before": ["<leader>", "e"],
        //     "commands": ["workbench.action.focusSideBar"]
        // },
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": ["<A-k>"],
            "commands": ["editor.action.moveLinesUpAction"]
        },
        {
            "before": ["<A-j>"],
            "commands": ["editor.action.moveLinesDownAction"]
        },
        {
            "before": ["<Up>"],
            "after": ["<NOP>"]
        },
        {
            "before": ["<Down>"],
            "after": ["<NOP>"]
        },
        {
            "before": ["<Left>"],
            "after": ["<NOP>"]
        },
        {
            "before": ["<Right>"],
            "after": ["<NOP>"]
        },
    ],
    // "workbench.colorTheme": "Noctis Obscuro",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.startupEditor": "newUntitledFile",
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features",
        "editor.tabSize": 4,
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features",
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 2,
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[python]": {
        "editor.formatOnType": true
    },
    "[scss]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features",
        "editor.tabSize": 2,
        "files.insertFinalNewline": true,
    },
    "git.openRepositoryInParentFolders": "never",
}
